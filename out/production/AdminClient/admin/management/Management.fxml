<AnchorPane prefHeight="400.0" stylesheets="@../app/Application.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admin.management.ManagementController">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="10.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="128.60001220703126" minHeight="0.0" prefHeight="50.00000190734863" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="266.8" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="336.60002746582035" minHeight="10.0" prefHeight="336.60002746582035" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
             <fx:include fx:id="UploadComponent" source="../upload/Upload.fxml" />
            <HBox GridPane.columnIndex="2">
               <children>
                  <Button fx:id="setThreadsCountButton" mnemonicParsing="false" onAction="#setThreadsCountButtonAction" text="Set threads Count">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <TextField fx:id="setThreadsCountTextField">
                     <HBox.margin>
                        <Insets right="5.0" top="12.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <ListView fx:id="threadPoolManagementListView" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <ListView fx:id="availableSimulationsDetailsListView" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
